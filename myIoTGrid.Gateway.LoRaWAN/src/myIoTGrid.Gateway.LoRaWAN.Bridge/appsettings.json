{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "myIoTGrid.Gateway.LoRaWAN.Bridge": "Debug",
      "MQTTnet": "Warning"
    }
  },
  "ChirpStack": {
    "MqttServer": "tcp://localhost:1883",
    "ClientId": "myiotgrid-bridge",
    "Topics": {
      "Uplink": "application/+/device/+/event/up",
      "Join": "application/+/device/+/event/join"
    }
  },
  "MyIoTGrid": {
    "MqttServer": "tcp://localhost:1884",
    "ClientId": "myiotgrid-gateway-lorawan",
    "Topics": {
      "Readings": "myiotgrid/readings/{nodeId}/{sensorType}",
      "NodesJoined": "myiotgrid/nodes/joined",
      "Status": "myiotgrid/status/gateway-lorawan"
    },
    "HubUrl": "http://localhost:5000"
  },
  "AllowedHosts": "*"
}
