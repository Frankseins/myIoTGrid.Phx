{
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://+:5001",
        "Certificate": {
          "Path": "/app/certs/fullchain.pem",
          "KeyPath": "/app/certs/privkey.pem"
        }
      }
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "*"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  }
}
