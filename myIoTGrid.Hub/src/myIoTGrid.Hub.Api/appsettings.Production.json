{
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://+:5001",
        "Certificate": {
          "Path": "/app/certs/fullchain.pem",
          "KeyPath": "/app/certs/privkey.pem"
        }
      },
      "Http": {
        "Url": "http://+:5000"
      },
      "Http2": {
        "Url": "http://+:5002"
      }
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "*"
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  }
}
